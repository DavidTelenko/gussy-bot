{
  "author": "DavidTelenko",
  "dependencies": {
    "telegraf": "^4.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.3",
    "@types/node": "^24.9.2",
    "cspell": "^9.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "typescript": "^5.9.3",
    "wrangler": "^4.46.0"
  },
  "license": "MIT",
  "name": "gussy-bot",
  "packageManager": "yarn@4.11.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && tsgo",
    "check": "biome check --write .",
    "clean": "rm -rf ./build",
    "deploy": "wrangler deploy --keep-vars",
    "dev": "wrangler dev",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "prepare": "husky",
    "set-webhook": "telegraf -m setWebhook",
    "spellcheck": "cspell --no-progress --no-must-find-files .",
    "typecheck": "tsgo --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
